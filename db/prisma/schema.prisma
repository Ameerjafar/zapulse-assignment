generator client {
  provider = "prisma-client-js"
   binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Action {
  id         Int        @id @default(autoincrement())
  name       String
  url        String
  method     String
  status     StatusType @default(PENDING)
  created_at DateTime   @default(now())
}

enum StatusType {
  PENDING
  SUCCESS
  FAILED
}
